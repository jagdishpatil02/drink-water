{
  "manifest_version": 2,
  "name": "Drink Water (Jal lijiye)",
  "description": "Drink Water (Jal lijiye) is chrome extension that will remind you drink water after every 20 mins.",
  "version": "1.0",
  "permissions": ["alarms"],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "icons": {
    "128": "./assets/icon.png",
    "48": "./assets/icon.png",
    "16": "./assets/icon.png"
  },
  "browser_action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "index.html"
  }
}
