{
  "manifest_version": 3,
  "name": "Drink Water (Jal Lijiye)",
  "description": "Drink Water (Jal Lijiye) is an extension that will remind you drink water after every 20 mins.",
  "version": "2.0",
  "permissions": ["alarms"],
  "icons": {
    "128": "./assets/icon.png",
    "48": "./assets/icon.png",
    "16": "./assets/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "index.html"
  }
}
